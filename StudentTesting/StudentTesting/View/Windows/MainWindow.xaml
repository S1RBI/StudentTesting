<Window x:Class="StudentTesting.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentTesting"
        MinHeight="300"
        MinWidth="550"
        Background="White"
        FontFamily="Times New Roman"
        mc:Ignorable="d"
        Title="{Binding ElementName=mainFrame, Path=Content.Title}" Height="450" Width="800">
    <Grid>
        <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
